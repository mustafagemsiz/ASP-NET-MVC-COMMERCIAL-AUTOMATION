@model List<Urun>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link href="https://fonts.googleapis.com/css?family=Quicksand:300,400,500,700,900" rel="stylesheet">
    <link rel="stylesheet" href="~/Files/thumber/fonts/icomoon/style.css">
    @*<link rel="stylesheet" href="~/Files/thumber/css/bootstrap.min.css">*@
    @*<link rel="stylesheet" href="~/Files/thumber/css/magnific-popup.css">*@
    @*<link rel="stylesheet" href="~/Files/thumber/css/jquery-ui.css">
    <link rel="stylesheet" href="~/Files/thumber/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/Files/thumber/css/owl.theme.default.min.css">*@
