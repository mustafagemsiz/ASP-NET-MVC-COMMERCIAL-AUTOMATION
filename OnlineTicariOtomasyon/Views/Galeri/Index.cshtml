    <link rel="stylesheet" href="~/Files/thumber/css/style.css">
    <div class="site-wrap">
        <main class="main-content">
            <div class="container-fluid photos">
                <div class="row align-items-stretch">
                    @foreach (var item in Model)
                    {
                        <div class="col-6 col-md-6 col-lg-4" data-aos="fade-up" data-aos-delay="100">
                            <a href="@item.UrunGorsel" class="d-block photo-item" data-fancybox="gallery">
                                <img src="@item.UrunGorsel" alt="@item.UrunAd" class="img-fluid">
                                <div class="photo-text-more">
                                    <span class="icon icon-search"></span>
                                </div>
                            </a>
                        </div>
                    }
                </div>
            </div>
        </main>
    </div> <!-- .site-wrap -->
    <script src="~/Files/thumber/js/jquery-3.3.1.min.js"></script>
